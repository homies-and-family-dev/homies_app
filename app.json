{
  "expo": {
    "name": "homies_app",
    "slug": "homiesapp",
    "version": "1.0.6",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.homiesandfamily.homiesapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "buildNumber": "1.0.6"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.homiesandfamily.homiesapp",
      "permissions": [
        "INTERNET",
        "GET_ACCOUNTS"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.514932886602-gcfrf3fcb6383ui6tbm2h28plqp52er5",
          "androidClientId": "514932886602-9l34ig1usu168d5dip17mkn63atjh8cr.apps.googleusercontent.com"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "74f2731c-27b0-456f-9ed2-b3cf6e8b4890"
      }
    },
    "owner": "soportehomies",
    "experiments": {
      "typedRoutes": true
    }
  }
}
